{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "rhd/vertical-slam",
	"version": "0.1.0",
	"title": "Vertical Slam!",
	"category": "layout",
	"icon": "star-half",
	"description": "A WordPress block for large vertical slam headings!",
	"supports": {
		"html": false
	},
	"textdomain": "rhd",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"text": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": ""
		}
	},
	"styles": [
		{ "name": "stick-left", "label": "Stick Left" },
		{ "name": "stick-right", "label": "Stick Right", "isDefault": true }
	]
}
