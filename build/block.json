{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "rhd/vertical-slam",
	"version": "0.1.0",
	"title": "Vertical Slam",
	"category": "layout",
	"icon": "star-half",
	"description": "Large vertical slam headings!",
	"supports": {
		"html": false
	},
	"textdomain": "vertical-slam",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"styles": [
		{ "name": "stick-page", "label": "Stick to page" },
		{ "name": "stick-content", "label": "Stick to content (below cover image)", "isDefault": true }
	],
	"attributes": {
		"text": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": ""
		}
	}
}
